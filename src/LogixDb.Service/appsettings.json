{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "EventLog": {
    "LogLevel": {
      "Default": "Information",
      "LogixDb.Service": "Information"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5088"
      }
    }
  },
  "LogixConfig": {
    "IngestionService": {
      "DropPath": "C:\\ProgramData\\LogixDb\\Uploads",
      "DbConnection": "logix@localhost",
      "OnImport": "ReplaceLatest"
    },
    "FtacService": {
      "Enabled": false,
      "Server": "localhost",
      "Database": "AssetCentre",
      "Port": 1433,
      "Filters": []
    }
  }
}
